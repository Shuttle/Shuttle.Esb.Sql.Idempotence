<?xml version="1.0"?>

<package>
  <metadata>
    <id>Shuttle.Esb.Sql.Idempotence</id>
    <version>{semver}</version>
    <authors>Eben Roux</authors>
    <owners>Eben Roux</owners>
    <license type="expression">BSD-3-Clause</license>
    <projectUrl>https://github.com/shuttle/Shuttle.Esb.Sql.Idempotence</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Shuttle/Shuttle.Esb.Sql.Idempotence/master/.media/logo.png</iconUrl>
    <repository type="git" url="https://github.com/Shuttle/Shuttle.Esb.Sql.Idempotence.git" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Sql-based implementation of IIdempotenceService interface for use with Shuttle.Esb.</description>
    <releaseNotes />
    <copyright>Copyright (c) {year}, Eben Roux</copyright>
    <tags>shuttle</tags>
    <dependencies>
      <group>
        <dependency id="Shuttle.Core.Data" version="{Shuttle.Core.Data-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
      </group>

